<div class="live-header-container">
  <% @events[0..2].each_with_index do |event, index| %>
    <div class="live-header-background" style="background: url('<%= event.images.first.url %>'); background-size:cover;">
      <div class="live-header">
        <div class="live-arrow carousel-left">
          <% unless index == 0 %>
            <i class="fa fa-chevron-left"></i>
          <% end %>
        </div>
        <div class="live-header-content">
          content
        </div>
        <div class="live-arrow carousel-right">
          <% unless index == 2 %>
            <i class="fa fa-chevron-right"></i>
          <% end %>
        </div>
      </div>
    </div>

  <% end %>
</div>

<div class="live-filler-container">
  <p>See All Events <i class="fa fa-arrow-down"></i></p>
</div>