<%= form_for venue do |f| %>
  <div>
    <%= f.label :name  %>
    <%= f.text_field :name %>
  </div>

  <div>
    <%= f.label :bio %>
    <%= f.text_area :bio %>
  </div>

   <div>
    <%= f.label :street  %>
    <%= f.text_field :street %>
  </div>

  <div>
    <%= f.label :city  %>
    <%= f.text_field :city %>
  </div>

  <div>
    <%= f.label :state  %>
    <%= f.text_field :state %>
  </div>

  <div>
    <%= label_tag :image, "Upload Images" %>
    <%= f.file_field :file, multiple: true  %>
  </div>

  <div>
    <%= f.submit %>
  </div>

<% end %>